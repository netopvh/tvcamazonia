    "desktopBestPriceSize": {
      "type": "string",
      "default": ""
    },
    "tabletBestPriceSize": {
      "type": "string",
      "default": ""
    },
    "mobileBestPriceSize": {
      "type": "string",
      "default": ""
    },
    "logoSize": {
      "type": "string",
      "default": ""
