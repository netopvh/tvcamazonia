    "tabletPriceSize": {
      "type": "string",
      "default": ""
    },
    "mobilePriceSize": {
      "type": "string",
      "default": ""
    },
    "priceColor": {
      "type": "string",
      "default": ""
    },
    "darkPriceColor": {
      "type": "string",
      "default": ""
    },
    "isButtonBorder": {
      "type": "boolean",
      "default": false
    },
    "desktopButtonSize": {
      "type": "string",
      "default": "12"
    },
    "tabletButtonSize": {
      "type": "string",
      "default": "12"
    },
    "mobileButtonSize": {
      "type": "string",
      "default": "12"
    },
    "buttonColor": {
      "type": "string",
      "default": ""
    },
    "buttonBg": {
      "type": "string",
      "default": ""
    },
    "buttonHoverColor": {
      "type": "string",
      "default": ""
    },
    "buttonHoverBg": {
      "type": "string",
      "default": ""
    },
    "darkButtonColor": {
      "type": "string",
      "default": ""
    },
    "darkButtonBg": {
      "type": "string",
      "default": ""
