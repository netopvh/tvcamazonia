    "borderColor": {
      "type": "string",
      "default": ""
    },
    "darkBorderColor": {
      "type": "string",
      "default": ""
    },
    "borderWidth": {
      "type": "object",
      "default": {
        "top": null,
        "right": null,
        "bottom": null,
        "left": null
      }
    },
    "borderRadius": {
      "type": "number"
    },
